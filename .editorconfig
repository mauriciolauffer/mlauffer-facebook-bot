# see http://editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{css,html,java,js,json,less,txt}]
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[pom.xml]
trim_trailing_whitespace = true
end_of_line = lf

[*.md]
trim_trailing_whitespace = false